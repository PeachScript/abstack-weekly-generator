    "casperjs": "^1.1.0",
    "chai": "^3.5.0",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-plugin-react": "^4.3.0",
    "mocha": "^2.4.5",
    "mocha-jsdom": "^1.1.0",
    "phantomjs-prebuilt": "^2.1.7",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
